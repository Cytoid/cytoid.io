{
  "name": "cytoid.io",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "description": "Cytoid is a music game where you can create, share and play your own levels! Powered by community, with many dedicated creators, Cytoid provides a huge variety of musical genres to enjoy and a diverse range of gameplay design.",
  "scripts": {
    "build": "pnpm test && nuxt build",
    "start": "node .output/server/index.mjs",
    "dev": "nuxt dev",
    "test": "pnpm tsc && pnpm lint",
    "dev:local": "cross-env NODE_APP_INSTANCE=local nuxt dev",
    "gql": "npx cross-env NODE_ENV=production node ./cli/codegen-update.js",
    "gql:local": "npx cross-env NODE_APP_INSTANCE=local node ./cli/codegen-update.js",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsc": "nuxi typecheck"
  },
  "dependencies": {
    "@headlessui/tailwindcss": "^0.2.1",
    "@headlessui/vue": "^1.7.23",
    "@nuxt/icon": "^1.10.1",
    "@nuxt/scripts": "0.9.5",
    "@nuxtjs/device": "^3.2.4",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@nuxtjs/turnstile": "^0.9.11",
    "@tailwindcss/typography": "^0.5.15",
    "@teages/nuxt-locale-lite": "^0.0.7",
    "@teages/nuxt-urql-client": "^2.0.8",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vueuse/components": "^12.0.0",
    "@vueuse/nuxt": "^12.0.0",
    "@wasm-audio-decoders/ogg-vorbis": "^0.1.16",
    "apexcharts": "^4.2.0",
    "daisyui": "^4.12.20",
    "date-fns": "^4.1.0",
    "easymde": "^2.18.0",
    "graphql": "^16.9.0",
    "howler": "^2.2.4",
    "marked": "^15.0.3",
    "nuxt": "^3.14.1592",
    "nuxt-gtag": "^3.0.2",
    "ogg-opus-decoder": "^1.6.14",
    "sanitize-html": "^2.13.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0"
