    "dotenv": "^16.4.5",
    "easymde": "^2.18.0",
    "eslint": "^9.4.0",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "howler": "^2.2.4",
    "marked": "^12.0.2",
    "nuxt": "^3.11.1",
    "nuxt-gtag": "^2.0.6",
    "nuxt-icon": "^0.6.8",
    "ogg-opus-decoder": "^1.6.12",
    "sanitize-html": "^2.12.1",
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "vue": "^3.4.27",
    "vue3-apexcharts": "^1.5.3"
  }
