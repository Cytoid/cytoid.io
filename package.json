    "@nuxt/devtools": "^1.6.3",
    "@types/config": "^3.3.5",
    "@types/glob": "^8.1.0",
    "@types/howler": "^2.2.12",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "@types/sanitize-html": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-legacy": "^5.4.3",
    "config": "^3.3.12",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
